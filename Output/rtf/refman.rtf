\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid    13 def compararLimites(data):\par
   14     #It represents the type of sensor unit\par
   15     unit =""\par
   16     #adjust values from data\par
   17     data['Tipo']=str(data['Tipo'])\par
   18     data['Valor']=float(data['Valor'])\par
   19     data['Ubicacion']=int(data['Ubicacion'])\par
   20     #get a phone list from bd\par
   21     phoneList=PhoneHandler.get_allEnable_Phones()\par
   22     #get max and min values from a type of sensor from bd\par
   23     limitMax=LimitHandler.get_Max_Value(data['Tipo'])\par
   24     limitMin=LimitHandler.get_min_Value(data['Tipo'])\par
   25     \par
   26     #adjust the unit based on the type of sensor\par
   27     if data['Tipo'] == 'temperatura':\par
   28         unit = "grados Centigrados"\par
   29     if data['Tipo'] == 'humedad':\par
   30         unit = "% humedad relativa"\par
   31     if data['Tipo'] == 'luz':\par
   32         unit = "luxes"\par
   33     if data['Tipo'] == 'co2':\par
   34         unit = "ppm"\par
   35     logging.info("Telefonos: ")\par
   36     for phone in phoneList:\par
   37         logging.info(phone.user_phone)\par
   38     logging.info("diccionario:")\par
   39     logging.info(data)\par
   40     logging.info("Limites- Tipo:"+str(type(limitMax))+" "+str(limitMax)+" ,Tipo: "+str(type(limitMin))+" "+str(limitMin))\par
   41 \par
   42     #compare whether the values are not within the established limits\par
   43 \par
   44     if data['Valor'] > limitMax and limitMax != None and limitMax != False:\par
   45         #build a message\par
   46         info= "La "+data["Tipo"] +" sobrepaso del limite establecido de "+ str(limitMax)+" "+unit+" Valor actual: "+str(data['Valor'])\par
   47         #send message to every phone on phone list\par
   48         for phone in phoneList:\par
   49             sms.sendMsg(str(phone.user_phone),info)\par
   50 \par
   51 \par
   52     if data['Valor'] < limitMin and limitMin != None and limitMin != False:\par
   53         #build a message\par
   54         info= "La "+data["Tipo"] +" esta por debajo del limite establecido de"+ str(limitMin)+" "+unit+" Valor actual: "+str(data['Valor'])\par
   55         #send message to every phone on phone list\par
   56         for phone in phoneList:\par
   57             sms.sendMsg(str(phone.user_phone),info)\par
   58 \par
   59     \par
   60 }
