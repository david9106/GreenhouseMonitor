\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid    18 def ajustar(sensor):\par
   19         if sensor['Tipo']=='Temperatura':\par
   20                 d1 = -39.6  # @var value used to get the real temperature measure\par
   21                 d2 = 0.01   # @var value used to get the real temperature\par
   22                 sensor['Valor']= d1+d2*sensor['Valor']\par
   23                 sensor['Valor'] =format(sensor['Valor'],'.2f')\par
   24                  \par
   25         if sensor['Tipo']=='Humedad':\par
   26                 c1 = -2.0468    # @var value used to get the real humidity \par
   27                 c2 = 0.0367     # @var value used to get the real humidity \par
   28                 c3 = -1.5955e-6 # @var value used to get the real humidity \par
   29                 sensor['Valor'] = c1 + c2 * sensor['Valor'] + c3 * math.pow(sensor['Valor'],2)\par
   30                 sensor['Valor'] =format(sensor['Valor'],'.2f')\par
   31                 \par
   32         if sensor['Tipo']=='Luz':\par
   33                 sensor['Valor'] = math.exp(3.3/(1024 * 0.56))*sensor['Valor']\par
   34                 sensor['Valor'] =format(sensor['Valor'],'.2f')\par
   35               \par
   36         if sensor['Tipo']=='co2':\par
   37                 sensor['Valor']=format(sensor['Valor'],'.2f')\par
   38 \par
   39 }
}
